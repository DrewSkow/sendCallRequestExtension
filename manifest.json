{
    "manifest_version": 3,
    "name": "Send Request For Phone Call",
    "description" : "Expansion for sending request",
    "version" : "0.0.1",
    "author": "Drew Skow",
    "content_scripts": [
        {
            "matches" : ["http://www.charmdate.com/clagt/lovecall/*"],
            "js"  : ["jquery.js","content.js"],
            "run_at": "document_end"
        }
    ],
    "action" : {
        "default_title" : "SendRequestBot",
        "default_icon" : "icon.png",
        "default_popup": "popup.html",
        "default_script": "popup.js"
    },

    "background" :{
        "service_worker" : "background.js"
    },


    "permissions" : [
        "activeTab",
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "http://www.charmdate.com/*"
    ],
    "web_accessible_resources": [{
        "resources": ["injected.js"],
        "matches": ["http://www.charmdate.com/*"]
    }]
}

